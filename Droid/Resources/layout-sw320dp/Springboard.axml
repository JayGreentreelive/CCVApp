            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#FF00FFFF" />
    </RelativeLayout>
<!--End springboard element definition-->
<!--Definition for a springboard element-->
    <RelativeLayout
        android:layout_below="@id/springboard_notes_frame"
        android:id="@+id/springboard_prayer_frame"
        android:layout_height="40dp"
        android:layout_width="match_parent">
        <Button
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/button" />
        <TextView
            android:text=""
            android:layout_height="wrap_content"
            android:layout_width="25dp"
            android:id="@+id/icon"
            android:layout_marginLeft="15dp"
            android:layout_centerVertical="true"
            android:gravity="center" />
        <TextView
            android:text="Prayer"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/text"
            android:layout_marginLeft="55dp"
            android:layout_centerVertical="true"
            android:layout_gravity="center_vertical" />
        <View
            android:id="@+id/seperator"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#FF5F5F5F" />
    </RelativeLayout>
<!--End springboard element definition-->
<!--Definition for a springboard element-->
    <RelativeLayout
        android:layout_below="@id/springboard_prayer_frame"
        android:id="@+id/springboard_give_frame"

    </RelativeLayout>
<!--End springboard element definition-->
    <View
        android:layout_below="@id/springboard_about_frame"
        android:id="@+id/end_seperator"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#FFF00FFF" />
<!--Bottom toolbar with Campus and Settings-->
    <RelativeLayout
        android:id="@+id/campus_container"
        android:background="@android:color/transparent"
        android:layout_height="40dp"
        android:layout_width="match_parent"
        android:layout_alignParentBottom="true">
        <Button
            android:id="@+id/campus_selection"
            android:text="EAST VALLEY CAMPUS"
            android:background="@android:color/transparent"
            android:layout_marginLeft="10dp"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:gravity="center" />
        <Button
            android:id="@+id/settings_button"
            android:text="Settings"
            android:background="@android:color/transparent"
            android:layout_height="match_parent"
            android:layout_width="25dp"
            android:layout_alignParentRight="true"
            android:gravity="center" />
    </RelativeLayout>
</RelativeLayout>
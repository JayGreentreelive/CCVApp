<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/springboard_relative_layout"
    android:layout_height="match_parent"
    android:layout_width="match_parent"
    android:minWidth="25px"
    android:minHeight="25px">
    <LinearLayout
        android:orientation="vertical"
        android:layout_marginTop="25dp"
        android:layout_marginLeft="0dp"
        android:id="@+id/springboard_profile_image_container"
        android:layout_height="wrap_content"
        android:layout_width="match_parent">
        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@android:color/transparent"
            android:id="@+id/springboard_profile_image_layout"
            android:layout_gravity="center_horizontal">
            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@android:color/transparent"
                android:text="W"
                android:id="@+id/springboard_profile_image"
                android:layout_centerInParent="true" />
        </RelativeLayout>
    <!--Relative layout that wraps the "Welcome: Jered" with a button surrounding it-->
        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="30dp"
            android:background="@android:color/transparent"
            android:layout_gravity="center_horizontal">
            <Button
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@android:color/transparent"
                android:id="@+id/springboard_login_button" />
            <LinearLayout
                android:background="@android:color/transparent"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:layout_centerInParent="true">
                <TextView
                    android:id="@+id/profile_prefix"
                    android:text="Welcome:"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
                <TextView
                    android:id="@+id/profile_name"
                    android:text="Jered"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content" />
            </LinearLayout>
        </RelativeLayout>
    <!--End "Welcome: Jered"-->
    </LinearLayout>
<!--Definition for a springboard element-->
    <RelativeLayout
        android:layout_below="@id/springboard_profile_image_container"
        android:id="@+id/springboard_news_frame"
        android:layout_marginTop="10dp"
        android:layout_height="40dp"
        android:layout_width="match_parent">
        <Button
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/button" />
        <TextView
            android:text=""
            android:layout_height="wrap_content"
            android:layout_width="25dp"
            android:id="@+id/icon"
            android:layout_marginLeft="15dp"
            android:layout_centerVertical="true"
            android:gravity="center" />
        <TextView
            android:text="News"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/text"
            android:layout_marginLeft="55dp"
            android:layout_centerVertical="true"
            android:layout_gravity="center_vertical" />
        <View
            android:id="@+id/seperator"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#FFF00FFF" />
    </RelativeLayout>
<!--End springboard element definition-->
<!--Definition for a springboard element-->
    <RelativeLayout
        android:layout_below="@id/springboard_news_frame"
        android:id="@+id/springboard_groupfinder_frame"
        android:layout_height="40dp"
        android:layout_width="match_parent">
        <Button
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/button" />
        <TextView
            android:text=""
            android:layout_height="wrap_content"
            android:layout_width="25dp"
            android:id="@+id/icon"
            android:layout_marginLeft="15dp"
            android:layout_centerVertical="true"
            android:gravity="center" />
        <TextView
            android:text="Connect"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/text"
            android:layout_marginLeft="55dp"
            android:layout_centerVertical="true"
            android:layout_gravity="center_vertical" />
        <View
            android:id="@+id/seperator"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#FFFFFF00" />
    </RelativeLayout>
<!--End springboard element definition-->
<!--Definition for a springboard element-->
